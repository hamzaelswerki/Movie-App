<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1500">
        <KeyFrameSet>
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="1"
                android:alpha="0.1"
                android:scaleY="0.1"
                android:scaleX="0.1" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="10"
                android:alpha="0.8"
                android:scaleX="0.8"
                android:scaleY="0.8" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="22"
                android:alpha="0.4"
                android:scaleX="0.6"
                android:scaleY="0.6" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="38"
                android:alpha="1"
                android:scaleX="0.8"
                android:scaleY="0.8" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="56"
                android:alpha="1"
                android:scaleX="1"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="66"
                android:scaleX="1" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="70"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="73"
                android:scaleY="1.2" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="76"
                android:scaleX="1.2" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="76"
                android:scaleX="1.2" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="79"
                android:scaleX="1.2" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="82"
                android:scaleY="0.8" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="85"
                android:scaleX="0.8" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="91"
                android:scaleX="1"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/lottie"
                motion:framePosition="98"
                android:translationY="30dp" />
            <KeyAttribute
                motion:motionTarget="@+id/texts"
                motion:framePosition="20"
                android:scaleX="0.6" />       <KeyAttribute
                motion:motionTarget="@+id/texts"
                motion:framePosition="20"
                android:scaleY="0.6" />
            <KeyAttribute
                motion:motionTarget="@+id/texts"
                motion:framePosition="60"
                android:scaleX="1" />
            <KeyAttribute
                motion:motionTarget="@+id/texts"
                motion:framePosition="60"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/texts"
                motion:framePosition="60"
                android:scaleX="1.2" /> <KeyAttribute
                motion:motionTarget="@+id/texts"
                motion:framePosition="60"
                android:scaleY="1.2" />
            <KeyAttribute
                motion:motionTarget="@+id/texts"
                motion:framePosition="60"
                android:scaleX="1" /> <KeyAttribute
            motion:motionTarget="@+id/texts"
            motion:framePosition="60"
            android:scaleY="1" />

        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            motion:layout_constraintVertical_bias="1.0"
            android:layout_height="@dimen/size_110"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.442"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="@dimen/size_110"
            android:id="@+id/lottie" />
        <Constraint
            android:layout_height="0dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/lottie"
            android:id="@+id/texts" />
        <Constraint
            android:layout_height="75dp"
            android:layout_width="70dp"
            motion:layout_constraintTop_toBottomOf="parent"
            motion:layout_constraintEnd_toStartOf="@+id/lottie"
            android:id="@+id/lottie2" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            motion:layout_constraintVertical_bias="0.57"
            android:layout_height="@dimen/size_110"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.546"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="@dimen/size_110"
            android:id="@+id/lottie" />
        <Constraint
            motion:layout_constraintVertical_bias="0.066"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/lottie"
            android:id="@+id/texts"
            motion:layout_constraintHorizontal_bias="0.538" />
        <Constraint
            android:layout_height="match_parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/imageViewForeground" />
        <Constraint
            android:layout_height="54dp"
            motion:layout_constraintEnd_toStartOf="@+id/texts"
            android:layout_width="42dp"
            android:id="@+id/lottie2"
            motion:layout_constraintTop_toBottomOf="@+id/lottie"
            motion:layout_constraintBottom_toBottomOf="@+id/texts" />
    </ConstraintSet>

</MotionScene>